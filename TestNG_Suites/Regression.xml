<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite parallel="tests" name="Suite">

<listeners>
	<listener class-name="SwagLabs.TestComponents.Listeners"/>
</listeners>


  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="SwagLabs.Tests.SubmittingOrderTests"/>
    </classes>     
  </test> 
  
  <test thread-count="5" name="Eror Validations Test">
   	<classes>
    	<class name="SwagLabs.Tests.ErrorValidationsTests"/>
    </classes>
  </test>  
  
  
  <test thread-count="5" name="Valid Login Test">
   	<classes>
    	<class name="SwagLabs.Tests.ValidLoginTests"/>
    </classes>
  </test>
  
   <test thread-count="5" name="Adding and Removing Products Functionality Test">
   	<classes>
    	<class name="SwagLabs.Tests.AddingAndRemovingProductsTest"/>
    </classes>
  </test>
  
  <test thread-count="5" name="Adding and Removing Products Functionality Test">
   	<classes>
    	<class name="SwagLabs.Tests.CheckingCartAfterLogoutTests"/>
    </classes>
  </test>

</suite> <!-- Suite -->
